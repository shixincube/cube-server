<?xml version="1.0" encoding="UTF-8"?>
<!-- Cell 容器配置文件 -->
<cell>
	<!-- 容器监听器 -->
	<listeners>
		<class>cube.ferryhouse.FerryhouseCellListener</class>
	</listeners>

	<!-- 内核参数配置 -->
	<nucleus>

		<!-- 对终端进行心跳保持请求的间隔，单位：毫秒 -->
		<heartbeat>60000</heartbeat>

		<!-- Talk 服务配置 -->
		<talk>
			<!-- 服务器参数配置 -->
			<server>
				<!-- 服务绑定的地址 -->
				<host>0.0.0.0</host>
				<!-- 服务绑定的端口号 -->
				<port>7979</port>
				<!-- 最大允许连接数 -->
				<max-connection>1000</max-connection>
			</server>

			<!-- SSL 配置 -->
			<ssl>
				<keystore>server.jks</keystore>
				<password>
					<store-password>shixincube.com</store-password>
					<manager-password>shixincube.com</manager-password>
				</password>
			</ssl>
		</talk>

		<!-- 日志配置 -->
		<log>
			<!-- 日志等级 -->
			<level>DEBUG</level>
		</log>
	</nucleus>

	<!-- Cellet 配置 -->
	<cellets>
		<cellet port="7979">
			<class>cube.ferryhouse.FerryhouseCellet</class>
		</cellet>
	</cellets>

	<!-- 适配器配置 -->
	<adapters></adapters>

</cell>
